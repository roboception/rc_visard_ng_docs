<ETHERNETKRL>
  <CONFIGURATION>
        <EXTERNAL>
          <IP>x.x.x.x</IP>
          <PORT>7000</PORT>
          <TYPE>Server</TYPE>
        </EXTERNAL>
        <INTERNAL>
          <BUFFERING Mode="FIFO" Limit="1"/>
          <ALIVE Set_Flag="997"/>
        </INTERNAL>
      </CONFIGURATION>
  <SEND>
    <XML>
      <ELEMENT Tag="req/node/rc_stereomatching" Type="STRING"/>
      <ELEMENT Tag="req/parameters/acquisition_mode/@value" Type="STRING"/>
      <ELEMENT Tag="req/parameters/double_shot/@value" Type="BOOL"/>
      <ELEMENT Tag="req/parameters/exposure_adapt_timeout/@value" Type="REAL"/>
      <ELEMENT Tag="req/parameters/fill/@value" Type="INT"/>
      <ELEMENT Tag="req/parameters/maxdepth/@value" Type="REAL"/>
      <ELEMENT Tag="req/parameters/maxdeptherr/@value" Type="REAL"/>
      <ELEMENT Tag="req/parameters/minconf/@value" Type="REAL"/>
      <ELEMENT Tag="req/parameters/mindepth/@value" Type="REAL"/>
      <ELEMENT Tag="req/parameters/quality/@value" Type="STRING"/>
      <ELEMENT Tag="req/parameters/seg/@value" Type="INT"/>
      <ELEMENT Tag="req/parameters/smooth/@value" Type="BOOL"/>
      <ELEMENT Tag="req/parameters/static_scene/@value" Type="BOOL"/>
      <ELEMENT Tag="req/end_of_request" Type="BOOL"/>
    </XML>
  </SEND>
  <RECEIVE>
    <XML>
      <ELEMENT Tag="res/parameters/acquisition_mode/@value" Type="STRING"/>
      <ELEMENT Tag="res/parameters/acquisition_mode/@default" Type="STRING"/>
      <ELEMENT Tag="res/parameters/acquisition_mode/@min" Type="STRING"/>
      <ELEMENT Tag="res/parameters/acquisition_mode/@max" Type="STRING"/>
      <ELEMENT Tag="res/parameters/double_shot/@value" Type="BOOL"/>
      <ELEMENT Tag="res/parameters/double_shot/@default" Type="BOOL"/>
      <ELEMENT Tag="res/parameters/double_shot/@min" Type="BOOL"/>
      <ELEMENT Tag="res/parameters/double_shot/@max" Type="BOOL"/>
      <ELEMENT Tag="res/parameters/exposure_adapt_timeout/@value" Type="REAL"/>
      <ELEMENT Tag="res/parameters/exposure_adapt_timeout/@default" Type="REAL"/>
      <ELEMENT Tag="res/parameters/exposure_adapt_timeout/@min" Type="REAL"/>
      <ELEMENT Tag="res/parameters/exposure_adapt_timeout/@max" Type="REAL"/>
      <ELEMENT Tag="res/parameters/fill/@value" Type="INT"/>
      <ELEMENT Tag="res/parameters/fill/@default" Type="INT"/>
      <ELEMENT Tag="res/parameters/fill/@min" Type="INT"/>
      <ELEMENT Tag="res/parameters/fill/@max" Type="INT"/>
      <ELEMENT Tag="res/parameters/maxdepth/@value" Type="REAL"/>
      <ELEMENT Tag="res/parameters/maxdepth/@default" Type="REAL"/>
      <ELEMENT Tag="res/parameters/maxdepth/@min" Type="REAL"/>
      <ELEMENT Tag="res/parameters/maxdepth/@max" Type="REAL"/>
      <ELEMENT Tag="res/parameters/maxdeptherr/@value" Type="REAL"/>
      <ELEMENT Tag="res/parameters/maxdeptherr/@default" Type="REAL"/>
      <ELEMENT Tag="res/parameters/maxdeptherr/@min" Type="REAL"/>
      <ELEMENT Tag="res/parameters/maxdeptherr/@max" Type="REAL"/>
      <ELEMENT Tag="res/parameters/minconf/@value" Type="REAL"/>
      <ELEMENT Tag="res/parameters/minconf/@default" Type="REAL"/>
      <ELEMENT Tag="res/parameters/minconf/@min" Type="REAL"/>
      <ELEMENT Tag="res/parameters/minconf/@max" Type="REAL"/>
      <ELEMENT Tag="res/parameters/mindepth/@value" Type="REAL"/>
      <ELEMENT Tag="res/parameters/mindepth/@default" Type="REAL"/>
      <ELEMENT Tag="res/parameters/mindepth/@min" Type="REAL"/>
      <ELEMENT Tag="res/parameters/mindepth/@max" Type="REAL"/>
      <ELEMENT Tag="res/parameters/quality/@value" Type="STRING"/>
      <ELEMENT Tag="res/parameters/quality/@default" Type="STRING"/>
      <ELEMENT Tag="res/parameters/quality/@min" Type="STRING"/>
      <ELEMENT Tag="res/parameters/quality/@max" Type="STRING"/>
      <ELEMENT Tag="res/parameters/seg/@value" Type="INT"/>
      <ELEMENT Tag="res/parameters/seg/@default" Type="INT"/>
      <ELEMENT Tag="res/parameters/seg/@min" Type="INT"/>
      <ELEMENT Tag="res/parameters/seg/@max" Type="INT"/>
      <ELEMENT Tag="res/parameters/smooth/@value" Type="BOOL"/>
      <ELEMENT Tag="res/parameters/smooth/@default" Type="BOOL"/>
      <ELEMENT Tag="res/parameters/smooth/@min" Type="BOOL"/>
      <ELEMENT Tag="res/parameters/smooth/@max" Type="BOOL"/>
      <ELEMENT Tag="res/parameters/static_scene/@value" Type="BOOL"/>
      <ELEMENT Tag="res/parameters/static_scene/@default" Type="BOOL"/>
      <ELEMENT Tag="res/parameters/static_scene/@min" Type="BOOL"/>
      <ELEMENT Tag="res/parameters/static_scene/@max" Type="BOOL"/>
      <ELEMENT Tag="res/return_code/@value" Type="INT"/>
      <ELEMENT Tag="res/return_code/@message" Type="STRING"/>
      <ELEMENT Tag="res" Set_Flag="998"/>
    </XML>
  </RECEIVE>
</ETHERNETKRL>
