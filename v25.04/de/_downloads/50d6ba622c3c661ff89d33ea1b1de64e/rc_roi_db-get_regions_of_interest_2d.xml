<ETHERNETKRL>
  <CONFIGURATION>
        <EXTERNAL>
          <IP>x.x.x.x</IP>
          <PORT>7000</PORT>
          <TYPE>Server</TYPE>
        </EXTERNAL>
        <INTERNAL>
          <BUFFERING Mode="FIFO" Limit="32"/>
          <ALIVE Set_Flag="997"/>
        </INTERNAL>
      </CONFIGURATION>
  <SEND>
    <XML>
      <ELEMENT Tag="req/node/rc_roi_db" Type="STRING"/>
      <ELEMENT Tag="req/service/get_regions_of_interest_2d" Type="STRING"/>
      <ELEMENT Tag="req/args/region_of_interest_2d_ids/le" Type="STRING"/>
      <ELEMENT Tag="req/end_of_request" Type="BOOL"/>
    </XML>
  </SEND>
  <RECEIVE>
    <XML>
      <ELEMENT Tag="res/regions_of_interest/le/height" Type="INT"/>
      <ELEMENT Tag="res/regions_of_interest/le/id" Type="STRING"/>
      <ELEMENT Tag="res/regions_of_interest/le/offset_x" Type="INT"/>
      <ELEMENT Tag="res/regions_of_interest/le/offset_y" Type="INT"/>
      <ELEMENT Tag="res/regions_of_interest/le/width" Type="INT"/>
      <ELEMENT Tag="res/return_code/@message" Type="STRING"/>
      <ELEMENT Tag="res/return_code/@value" Type="INT"/>
      <ELEMENT Tag="res" Set_Flag="998"/>
    </XML>
  </RECEIVE>
</ETHERNETKRL>
