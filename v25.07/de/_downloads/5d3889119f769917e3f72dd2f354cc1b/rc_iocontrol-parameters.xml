<ETHERNETKRL>
  <CONFIGURATION>
        <EXTERNAL>
          <IP>x.x.x.x</IP>
          <PORT>7000</PORT>
          <TYPE>Server</TYPE>
        </EXTERNAL>
        <INTERNAL>
          <BUFFERING Mode="FIFO" Limit="1"/>
          <ALIVE Set_Flag="997"/>
        </INTERNAL>
      </CONFIGURATION>
  <SEND>
    <XML>
      <ELEMENT Tag="req/node/rc_iocontrol" Type="STRING"/>
      <ELEMENT Tag="req/parameters/out1_inverted/@value" Type="BOOL"/>
      <ELEMENT Tag="req/parameters/out1_mode/@value" Type="STRING"/>
      <ELEMENT Tag="req/parameters/out1_ratio/@value" Type="REAL"/>
      <ELEMENT Tag="req/parameters/out2_inverted/@value" Type="BOOL"/>
      <ELEMENT Tag="req/parameters/out2_mode/@value" Type="STRING"/>
      <ELEMENT Tag="req/parameters/out2_ratio/@value" Type="REAL"/>
      <ELEMENT Tag="req/end_of_request" Type="BOOL"/>
    </XML>
  </SEND>
  <RECEIVE>
    <XML>
      <ELEMENT Tag="res/parameters/out1_inverted/@value" Type="BOOL"/>
      <ELEMENT Tag="res/parameters/out1_inverted/@default" Type="BOOL"/>
      <ELEMENT Tag="res/parameters/out1_inverted/@min" Type="BOOL"/>
      <ELEMENT Tag="res/parameters/out1_inverted/@max" Type="BOOL"/>
      <ELEMENT Tag="res/parameters/out1_mode/@value" Type="STRING"/>
      <ELEMENT Tag="res/parameters/out1_mode/@default" Type="STRING"/>
      <ELEMENT Tag="res/parameters/out1_mode/@min" Type="STRING"/>
      <ELEMENT Tag="res/parameters/out1_mode/@max" Type="STRING"/>
      <ELEMENT Tag="res/parameters/out1_ratio/@value" Type="REAL"/>
      <ELEMENT Tag="res/parameters/out1_ratio/@default" Type="REAL"/>
      <ELEMENT Tag="res/parameters/out1_ratio/@min" Type="REAL"/>
      <ELEMENT Tag="res/parameters/out1_ratio/@max" Type="REAL"/>
      <ELEMENT Tag="res/parameters/out2_inverted/@value" Type="BOOL"/>
      <ELEMENT Tag="res/parameters/out2_inverted/@default" Type="BOOL"/>
      <ELEMENT Tag="res/parameters/out2_inverted/@min" Type="BOOL"/>
      <ELEMENT Tag="res/parameters/out2_inverted/@max" Type="BOOL"/>
      <ELEMENT Tag="res/parameters/out2_mode/@value" Type="STRING"/>
      <ELEMENT Tag="res/parameters/out2_mode/@default" Type="STRING"/>
      <ELEMENT Tag="res/parameters/out2_mode/@min" Type="STRING"/>
      <ELEMENT Tag="res/parameters/out2_mode/@max" Type="STRING"/>
      <ELEMENT Tag="res/parameters/out2_ratio/@value" Type="REAL"/>
      <ELEMENT Tag="res/parameters/out2_ratio/@default" Type="REAL"/>
      <ELEMENT Tag="res/parameters/out2_ratio/@min" Type="REAL"/>
      <ELEMENT Tag="res/parameters/out2_ratio/@max" Type="REAL"/>
      <ELEMENT Tag="res/return_code/@value" Type="INT"/>
      <ELEMENT Tag="res/return_code/@message" Type="STRING"/>
      <ELEMENT Tag="res" Set_Flag="998"/>
    </XML>
  </RECEIVE>
</ETHERNETKRL>
